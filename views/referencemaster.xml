<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_reference_master_form" model="ir.ui.view">
        <field name="name">reference_master.form</field>
        <field name="model">referance_master</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="No"/>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_reference_master_tree" model="ir.ui.view">
        <field name="name">reference_master.tree</field>
        <field name="model">referance_master</field>
        <field name="arch" type="xml">
            <tree>
                <field name="No"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_reference_master" model="ir.actions.act_window">
        <field name="name">Reference Master</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">referance_master</field>
        <field name="view_mode">kanban,tree,form</field>
    </record>
</odoo>