<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_purchaseorder_new" model="ir.sequence">
        <field name="name">Purchaseorder Sequence</field>
        <field name="code">purchase.order.new</field>
        <field name="prefix">PO</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_vpo" model="ir.sequence">
        <field name="name">Vpo Sequence</field>
        <field name="code">vpo.seq</field>
        <field name="prefix">VPO</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="seq_gpo_line" model="ir.sequence">
        <field name="name">GPO Line Sequence</field>
        <field name="code">lbx_mi_gpo_d001_sequence</field>
        <field name="prefix">GPO-L-</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
        <field name="implementation">no_gap</field>
    </record>


</odoo>
